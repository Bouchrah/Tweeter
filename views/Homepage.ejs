

     <%- include ('./header')%>

    <%- include('./navbaar') %>
    <title><%= pageTitle %></title>
      
     
     
    

    <% if (articelArr.length > 0) { %>
                <%articelArr.forEach((article) =>{ %>
                        <div class="card" style="width: 18rem;">
                        <div class="card-body">
                            
                            <a class="See" href="/showarticle/<%=article.id %>"> See more</a>
                            <h5 class="card-title"><%=article.title%></h5>
                           
                            <p class="card-text"><%=article.article%></p>
                        </div>
                        </div>
                
                <% }) %>
    
    <% } else { %>
        <h5> No More tweet To Display</h5>
    <% } %>
  
<%- include('./Footer') %>
    


